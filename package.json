	"private": true,
	"type": "module",
	"scripts": {
		"preinstall": "pnpx only-allow pnpm",
		"dev": "vite dev",
		"build": "tsc -b && vite build",
		"start": "vite preview",
		"types:vite-env": "pnpx tsx scripts/generate-vite-env.ts",
		"types:db": "pnpx supabase gen types typescript --project-id \"uguvrytefzvyjypcbngo\" --schema public > src/types/database.types.ts && prettier --write src/types/database.types.ts",
		"types:cf": "wrangler types",
		"types": "pnpm types:vite-env && pnpm types:cf && pnpm types:db",
		"lint:check": "eslint . --max-warnings 0",
		"lint:fix": "eslint . --fix",
		"format:check": "prettier --check .",
		"format:fix": "prettier --write .",
		"type:check": "tsc --noEmit",
		"check": "pnpm type:check && pnpm lint:check && pnpm format:check",
		"fix": "pnpm lint:fix && pnpm format:fix",
		"test": "vitest run",
		"deploy": "wrangler deploy",
		"prepare": "pnpx is-ci || husky"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix"
		],
		"*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
